<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <Match>
        <Source name="~.*Test\.java" />
    </Match>
    <Match>
        <Class name="~.*SecurityConfig.*" />
        <Bug pattern="SE_BAD_FIELD,SE_NO_SERIALVERSIONID" />
    </Match>
    <Match>
        <Bug pattern="EI_EXPOSE_REP,EI_EXPOSE_REP2" />
    </Match>
</FindBugsFilter>
